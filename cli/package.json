{
  "name": "@usys/cli",
  "version": "0.0.1",
  "description": "Unbounded Adapt CLI",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "author": "Unbounded Systems",
  "bin": {
    "adapt": "./bin/run"
  },
  "bugs": {
    "url": "https://gitlab.com/unboundedsystems/adapt/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.4.33",
    "@oclif/config": "^1.6.28",
    "@oclif/plugin-help": "^2.0.5",
    "chalk": "^2.4.1",
    "decamelize": "^2.0.0",
    "fs-extra": "^6.0.1",
    "listr": "^0.14.1",
    "lodash": "^4.17.10",
    "pacote": "^8.1.6",
    "semver": "^5.5.0",
    "signal-exit": "^3.0.2",
    "source-map-support": "^0.5.6",
    "ts-custom-error": "^2.2.1",
    "tslib": "^1.9.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.31",
    "@oclif/test": "^1.1.0",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chalk": "^2.2.0",
    "@types/decamelize": "^1.2.0",
    "@types/fs-extra": "^5.0.3",
    "@types/listr": "^0.13.0",
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^5.2.1",
    "@types/node": "^8.10.20",
    "@types/semver": "^5.5.0",
    "@usys/testutils": "0.0.1",
    "@usys/utils": "0.0.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "fancy-test": "^1.2.0",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "postinstall-build": "^5.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "peerDependencies": {},
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://gitlab.com/unboundedsystems/adapt",
  "license": "UNLICENSED",
  "oclif": {
    "bin": "adapt",
    "commands": "./dist/src/commands",
    "description": "Command line interface for Adapt",
    "dirname": "adapt",
    "hooks": {
      "init": "./dist/src/hooks/init"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/unboundedsystems/adapt.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "clean": "rm -rf dist",
    "coverage": "nyc mocha --opts mocha.opts",
    "lint": "tslint --project .",
    "lint-and-fix": "tslint --project . --fix",
    "postinstall": "postinstall-build --only-as-dependency dist",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "make build && oclif-dev manifest && oclif-dev readme",
    "pretest": "make build",
    "test": "mocha --opts mocha.opts",
    "version": "oclif-dev readme && git add README.md"
  }
}
