{
  "name": "@usys/cli",
  "version": "0.0.1",
  "description": "Unbounded Adapt CLI",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "author": "Unbounded Systems",
  "bin": {
    "adapt": "./bin/run"
  },
  "bugs": {
    "url": "https://gitlab.com/unboundedsystems/adapt/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.4.33",
    "@oclif/config": "^1.6.28",
    "@oclif/plugin-help": "^2.0.5",
    "tslib": "^1.9.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.31",
    "@oclif/test": "^1.1.0",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.2",
    "husky": "^0.14.3",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "ts-node": "^6.1.1",
    "postinstall-build": "^5.0.1",
    "source-map-support": "^0.5.6",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "peerDependencies": {
    "@usys/adapt": "*"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://gitlab.com/unboundedsystems/adapt",
  "license": "UNLICENSED",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "adapt",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/unboundedsystems/adapt.git"
  },
  "scripts": {
    "build": "npm run clean; npm run _build",
    "_build": "tsc -p tsconfig.json",
    "build:watch": "npm run clean; npm run _build:watch",
    "_build:watch": "tsc -w -p tsconfig.json",
    "clean": "rm -rf dist",
    "lint": "tslint --project .",
    "lint-and-fix": "tslint --project . --fix",
    "postinstall": "postinstall-build dist",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf dist && tsc && oclif-dev manifest && oclif-dev readme",
    "prepare": "npm run build",
    "prepush": "../bin/npm run lint && ../bin/npm run test",
    "pretest": "npm run build",
    "test": "nyc mocha --opts test/mocha.opts",
    "version": "oclif-dev readme && git add README.md"
  }
}
