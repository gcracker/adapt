    "@types/fs-extra": "8.1.1",
    "@types/graphlib": "2.1.7",
    "@types/graphql-type-json": "0.3.2",
    "@types/indent-string": "3.2.0",
    "@types/json-stable-stringify": "1.0.32",
    "@types/lodash": "4.14.165",
    "@types/mkdirp": "0.5.2",
