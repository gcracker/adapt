  "repository": "unboundedsystems/adapt",
  "bugs": {
    "url": "https://gitlab.com/unboundedsystems/adapt/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "adapt",
    "adaptjs",
    "adpt",
    "unbounded",
    "infrastructure-as-code",
    "devops",
    "deploy",
    "aws",
    "k8s",
