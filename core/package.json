    "graphql": "14.7.0",
    "graphql-tag": "2.11.0",
    "graphql-tools": "4.0.8",
    "graphql-type-json": "0.3.2",
    "indent-string": "3.2.0",
    "json-stable-stringify": "1.0.1",
    "lodash": "4.17.20",
    "mkdirp": "0.5.5",
    "moment": "2.24.0",
    "node-json-db": "1.1.0",
    "p-defer": "3.0.0",
    "p-queue": "4.0.0",
    "p-settle": "2.1.0",
