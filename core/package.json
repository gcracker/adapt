    "@types/p-settle": "2.0.1",
    "@types/proper-lockfile": "4.1.1",
    "@types/randomstring": "1.1.6",
    "@types/read-pkg-up": "3.0.1",
    "@types/sinon": "9.0.8",
    "@types/type-name": "1.0.28",
    "execa": "4.0.3",
    "fs-extra": "9.0.1",
    "mocha": "8.2.0",
    "mocha-slow-options": "0.0.2",
    "should": "13.2.3",
    "sinon": "9.2.0",
    "source-map-support": "0.5.19",
    "type-ops": "3.0.3",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "@adpt/dom-parser": "0.4.0-next.9",
    "@adpt/utils": "0.4.0-next.9",
    "@usys/fork-require": "1.0.9-unb3",
    "async-lock": "1.2.4",
    "callsites": "3.1.0",
    "css-what": "4.0.0",
    "debug": "4.2.0",
    "flush-write-stream": "2.0.0",
